<tree string="Franchise Price List" editable="bottom">
    <field name="product"/>
    <field name="product_type"/>
    <field name="franchise_name"/>
    <button name="set_franchises" string="Set Franchises"/>
    <field name="product_cost_price"/>
    <field name="quantity"/>
    <field name="sale_percent" factor="100.0"/>
    <field name="sale_price"/>
    <field name="sale_price_with_vat"/>
    <field name="public_percent" factor="100.0"/>
    <field name="public_price"/>
    <field name="franchises" tree_invisible="1"/>
</tree>
